    "gh-pages": "^5.0.0",
    "imagesloaded": "^5.0.0",
    "lightgallery": "^2.7.2",
    "markdown-to-jsx": "^7.1.9",
    "masonry-layout": "^4.2.2",
    "prop-types": "^15.8.1",
    "raw.macro": "^0.4.2",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
